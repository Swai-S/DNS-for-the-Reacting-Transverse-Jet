FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;

boundaryField
{
    jetinlet
    {
        type            zeroGradient;
    }

    airinlet
    {
        type             zeroGradient;
    }

    outlet
    {
        type            totalPressure;
        p0              $internalField;
    }

    farfield
    {
        type           totalPressure;
        p0             $internalField;
    }

    plate
    {
        type            zeroGradient;
    }

    frontAndBackPlanes
    {
        type            empty;
    }
}

// ************************************************************************* //

